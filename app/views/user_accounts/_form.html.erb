<div>
  <%= form.label "Name" %>
  <%= form.text_field :name %>
</div>
<div>
  <%= form.label "Email" %>
  <%= form.text_field :email %>
</div>
<div>
  <%= form.label "Role" %>
  <%= form.select :role, ["admin","publisher","master"], include_blank: true %>
</div>
<div>
  <%= form.submit "Save" %>
</div>
